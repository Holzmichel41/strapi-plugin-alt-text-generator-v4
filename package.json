  },
  "maintainers": [
    {
      "name": "holzmichel41 <47560723+Holzmichel41@users.noreply.github.com>"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "develop": "tsc -p tsconfig.server.json -w",
    "build": "pack-up build",
    "watch": "pack-up watch",
    "check": "pack-up check"
  },
  "files": [
    "dist",
    "strapi-admin.js",
    "strapi-server.js"
  ],
